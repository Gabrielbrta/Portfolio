{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-inner-declarations":"off",
        "no-param-reassign": "off",
        "no-console": "off",
        "import/extensions": 0,
        "no-multi-assign": "off"
    }
}
